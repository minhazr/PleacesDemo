<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.minhaz.placesdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="info.minhaz.placesdemo.MapsActivity" android:label="@string/title_activity_maps"></activity>

        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/PLACES_API_KEY"/>
        <provider
            android:name=".model.AppContentProvider"
            android:authorities="info.minhaz.placesdemo.model.AppContentProvider"
            android:enabled="true"
            android:exported="false" />
    </application>

</manifest>